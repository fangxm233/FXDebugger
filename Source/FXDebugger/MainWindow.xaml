<Window x:Class="FXDebugger.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FXDebugger"
        mc:Ignorable="d"
        Title="FXDebugger" Height="350" Width="525">
    <Grid Background="#FF151515">
       
        <Button x:Name="Continue" Content="Continue" HorizontalAlignment="Left" VerticalAlignment="Top" Width="195" Click="ContinueButton_Click" Height="30"/>
        <TextBox x:Name="Output" Margin="200,0,0,0" TextWrapping="Wrap" ScrollViewer.VerticalScrollBarVisibility="Auto" ScrollViewer.HorizontalScrollBarVisibility="Auto" Background="Black" Foreground="White" OpacityMask="White" BorderBrush="{x:Null}" IsReadOnly="True"/>
        <Button x:Name="About" Content="About" HorizontalAlignment="Left" VerticalAlignment="Top" Width="195" Click="AboutButton_Click" Height="30" Margin="0,30,0,0"/>

    </Grid>
</Window>
